{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 4000",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx,.astro src",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "if [[ $NODE_ENV != \"production\" ]]; then cd .. && husky install client/.husky; fi",
    "precommit": "npm run format && npm run lint:fix"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.1",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-transition-group": "^4.4.5",
    "@types/yup": "^0.32.0",
    "formik": "^2.2.9",
    "node-sass": "^8.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.41.3",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.4.5",
    "react-transition-group": "^4.4.5",
    "sass": "^1.56.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/node": "^18.11.13",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.20",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  }
}
